        "\n",
        "INNER JOIN (Users with Login Logs)\n",
        "query = \"\"\"\n",
        "SELECT u.user_id, u.username, l.login_time, l.ip_address\n",
        "FROM users u\n",
        "INNER JOIN login_logs l\n",
        "ON u.user_id = l.user_id;\n",
        "\"\"\"\n",
        "\n",
        "pd.read_sql_query(query, conn)"
