        "FULL OUTER JOIN\n",
        "query = \"\"\"\n",
        "SELECT u.username, l.login_time\n",
        "FROM users u\n",
        "LEFT JOIN login_logs l ON u.user_id = l.user_id\n",
        "UNION\n",
        "SELECT u.username, l.login_time\n",
        "FROM login_logs l\n",
        "LEFT JOIN users u ON u.user_id = l.user_id;\n",
        "\"\"\"\n",
        "\n",
        "pd.read_sql_query(query, conn)\n"
      ],
