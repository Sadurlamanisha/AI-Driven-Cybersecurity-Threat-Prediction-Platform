      "cell_type": "code",
      "source": [
        "\n",
        "LEFT JOIN (Users with Alerts)\n",
        "query = \"\"\"\n",
        "SELECT u.username, a.alert_type, a.severity\n",
        "FROM users u\n",
        "LEFT JOIN alerts a\n",
        "ON u.user_id = a.user_id;\n",
        "\"\"\"\n",
        "\n",
        "pd.read_sql_query(query, conn)"
